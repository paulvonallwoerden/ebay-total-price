{
    "manifest_version": 3,
    "name": "Ebay Total Price",
    "description": "Shows the total price of an Ebay item including shipping",
    "version": "1.0",
    "content_scripts": [
        {
            "js": ["extension.js"],
            "matches": [
                "https://www.ebay.de/*",
                "https://www.ebay.com/*"
            ]
        }
    ]
}
